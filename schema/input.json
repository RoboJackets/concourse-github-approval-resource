{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "properties": {
        "pull_request_url": {
            "type": "string",
            "format": "uri"
        },
        "commit_id": {
            "type": "string",
            "regex": "[a-z0-9]{40}"
        },
        "token": {
            "type": "string",
            "regex": "ghs_[a-zA-Z0-9]{36}"
        },
        "debug": {
            "const": true
        }
    },
    "required": [
        "pull_request_url",
        "commit_id",
        "token"
    ]
}
